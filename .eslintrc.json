{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    // "plugin:react/recommended"
    "airbnb"
  ],
  // "parser": "babel-eslint",
  "parserOptions": {
    "parser": "babel-eslint",
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ]
  }
}